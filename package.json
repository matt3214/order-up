{
  "name": "electron-react-env",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ionic/core": "^4.11.1",
    "@ionic/react": "^4.11.1",
    "@ionic/react-router": "^4.11.1",
    "@types/jest": "24.0.19",
    "@types/node": "12.11.1",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.0",
    "cross-env": "^6.0.3",
    "electron-is-dev": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "ionic": "^5.4.4",
    "ionic-react": "^0.0.0",
    "ionicons": "^4.6.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "typescript": "^3.6.4"
  },
  "description": "electron-react-ionic template app",
  "author": "Matthew Duffy",
  "build": {
    "appId": "<com.your_app>"
  },
  "main": "public/electron.js",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "clean": "rm -rf dist/ && rm -rf build/",
    "release": "yarn react-build && electron-builder --publish=always",
    "build": "yarn react-build && yarn electron-build",
    "start": "concurrently --kill-others --success first --names \"BACKEND,FRONTEND\" \"cross-env BROWSER=none yarn react-start\" \"wait-on http://localhost:3000 && electron .\" && exit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.0.0",
    "electron": "^7.2.4",
    "electron-builder": "^21.2.0",
    "wait-on": "^3.3.0"
  }
}
